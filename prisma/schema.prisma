generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model planets {
  id         Int     @id @default(autoincrement())
  name       String
  mass       Float
  hasStation Boolean
}

model users {
  id       Int    @id @default(autoincrement())
  email    String @unique
  password String
}